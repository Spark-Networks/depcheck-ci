{
  "name": "depcheck-ci",
  "version": "1.0.0",
  "engines": {
    "node": ">= 6.2.1"
  },
  "description": "Depcheck util that plays nice with ci",
  "author": "mcasimir",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -Rf ./lib",
    "build": "babel src -d lib",
    "release": "release-flow",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "BABEL_ENV=test nyc -r lcov -r text mocha --compilers js:babel-register --require babel-polyfill 'test/unit/**/*.spec.js'",
    "test:integration": "BABEL_ENV=test nyc -r lcov -r text mocha --compilers js:babel-register --require babel-polyfill 'test/integration/**/*.spec.js'",
    "test:lint": "eslint . --quiet",
    "test:ci": "npm run test:lint && npm test",
    "depcheck": "npm run build && node ./lib/cli.js"
  },
  "bin": {
    "depcheck-ci": "./lib/cli.js"
  },
  "nyc": {
    "include": [
      "lib/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "chalk": "1.1.3",
    "yargs": "5.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.11.4",
    "babel-eslint": "6.1.2",
    "babel-plugin-istanbul": "1.0.3",
    "babel-plugin-syntax-async-functions": "6.8.0",
    "babel-plugin-transform-builtin-extend": "1.1.0",
    "babel-plugin-transform-class-properties": "6.11.5",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-assign": "6.8.0",
    "babel-plugin-transform-regenerator": "6.11.4",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-0": "6.5.0",
    "eslint": "3.1.1",
    "eslint-config-google": "0.6.0",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-mocha": "4.2.0",
    "mocha": "2.5.3",
    "nodemon": "1.10.0",
    "npm-run-all": "2.3.0",
    "nyc": "7.1.0",
    "release-flow": "1.2.1",
    "sinon": "1.17.5"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mcasimir/depcheck-ci.git"
  }
}
